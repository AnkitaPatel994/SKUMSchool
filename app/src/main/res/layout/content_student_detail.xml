<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.intelliworkz.skumschool.StudentDetail.StudentDetailActivity"
    tools:showIn="@layout/app_bar_student_detail">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/img_Attendance"
            android:layout_width="200dp"
            android:layout_height="200dp"
            android:background="@drawable/attendance"
            app:srcCompat="@drawable/gradeient_bg"/>

    </LinearLayout>

</LinearLayout>
